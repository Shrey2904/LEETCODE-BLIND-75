   "source": [
    "def reverseLinkedlist(head):\n",
    "    \n",
    "    prev, curr = None, head\n",
    "    \n",
    "    while curr:\n",
    "        \n",
    "        nxt = curr.next\n",
    "        curr.next = prev\n",
    "        prev = curr\n",
    "        curr = nxt\n",
    "    return prev"
   ]
