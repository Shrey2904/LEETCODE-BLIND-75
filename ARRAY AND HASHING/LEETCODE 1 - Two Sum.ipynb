   "source": [
    "nums = [2,7,11,15]\n",
    "target = 22\n",
    "\n",
    "def twosum(nums, target):\n",
    "    d = {}\n",
    "    \n",
    "    for i, n in enumerate(nums):\n",
    "        diff = target - n\n",
    "        if diff in d:\n",
    "            return (d[diff], i)\n",
    "        d[n] = i\n",
    "twosum(nums, target)"
   ]
